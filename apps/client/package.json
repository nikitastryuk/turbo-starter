{
  "name": "client",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "npm run with-env next build",
    "clean": "git clean -xdf .next .turbo node_modules",
    "dev": "npm run with-env next dev",
    "format": "prettier --check \"**/*.{js,cjs,mjs,ts,tsx,md,json}\"",
    "lint": "eslint . --fix",
    "ncu": "ncu -u",
    "start": "next start",
    "typecheck": "tsc --pretty --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "eslintConfig": {
    "extends": [
      "@llmaid/eslint-config/base",
      "@llmaid/eslint-config/nextjs"
    ],
    "root": true
  },
  "dependencies": {
    "next": "14.0.4",
    "next-intl": "^3.4.0",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@t3-oss/env-nextjs": "^0.7.1",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.16",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.0.4",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3"
  }
}
