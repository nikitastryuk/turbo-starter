{
  "name": "client",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "npm run with-env next build",
    "clean": "git clean -xdf .next .turbo node_modules",
    "dev": "npm run with-env next dev",
    "format": "prettier --check \"**/*.{js,cjs,mjs,ts,tsx,md,json}\"",
    "lint": "eslint . --fix",
    "ncu": "ncu -u",
    "start": "next start",
    "typecheck": "tsc --pretty --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "eslintConfig": {
    "extends": [
      "@llmaid/eslint-config/base",
      "@llmaid/eslint-config/nextjs"
    ],
    "root": true
  },
  "dependencies": {
    "next": "14.0.1",
    "next-intl": "^3.0.0-rc.9",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@t3-oss/env-nextjs": "^0.7.1",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "autoprefixer": "^10.4.16",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.53.0",
    "eslint-config-next": "14.0.1",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2"
  }
}
